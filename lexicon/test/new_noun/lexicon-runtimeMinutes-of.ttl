@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base   <http://localhost:8080#> .

:lexicon_en
    a              lemon:Lexicon ;
    lemon:language "en" ;
    lemon:entry    :movie_of ;
    lemon:entry    :of .

:movie_of
    a                    lemon:LexicalEntry ;
    lexinfo:partOfSpeech lexinfo:noun ;
    lemon:canonicalForm  :movie_form ;
    lemon:synBehavior    :movie_of_nounpp ;
    lemon:sense          :movie_sense_ontomap .

:movie_form
    a                lemon:Form ;
    lemon:writtenRep "movie"@en .

:movie_of_nounpp
    a                            lexinfo:NounPPFrame ;
    lexinfo:copulativeArg        :arg1 ;
    lexinfo:prepositionalAdjunct :arg2 .

:movie_sense_ontomap
    a                 lemon:OntoMap, lemon:LexicalSense ;
    lemon:ontoMapping :movie_sense_ontomap ;
    lemon:reference   <http://dbpedia.org/ontology/Person> ;
    lemon:subjOfProp  :arg2 ;
    lemon:objOfProp   :arg1 ;
    lemon:condition   :movie_condition .

:movie_condition
    a                    lemon:condition ;
    lemon:propertyDomain <http://dbpedia.org/ontology/Person> ;
    lemon:propertyRange  <http://dbpedia.org/ontology/Work> .

:arg2
    lemon:marker :of .

## Prepositions ##

:of
    a                    lemon:SynRoleMarker ;
    lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
    lexinfo:partOfSpeech lexinfo:preposition .
