@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base   <http://localhost:8080#> .

:lexicon_en
    a              lemon:Lexicon ;
    lemon:language "en" ;
    lemon:entry    :bSide_of ;
    lemon:entry    :of .

:bSide_of
    a                    lemon:LexicalEntry ;
    lexinfo:partOfSpeech lexinfo:noun ;
    lemon:canonicalForm  :bSide_form ;
    lemon:synBehavior    :bSide_of_nounpp ;
    lemon:sense          :bSide_sense_ontomap .

:bSide_form
    a                lemon:Form ;
    lemon:writtenRep "B-side"@en .

:bSide_of_nounpp
    a                            lexinfo:NounPPFrame ;
    lexinfo:copulativeArg        :arg1 ;
    lexinfo:prepositionalAdjunct :arg2 .

:bSide_sense_ontomap
    a                 lemon:OntoMap, lemon:LexicalSense ;
    lemon:ontoMapping :bSide_sense_ontomap ;
    lemon:reference   <http://dbpedia.org/ontology/bSide> ;
    lemon:subjOfProp  :arg2 ;
    lemon:objOfProp   :arg1 ;
    lemon:condition   :bSide_condition .

:bSide_condition
    a                    lemon:condition ;
    lemon:propertyDomain <http://dbpedia.org/ontology/Song> ;
    lemon:propertyRange  <https://www.w3.org/2001/XMLSchema#string> .

:arg2
    lemon:marker :of .

## Prepositions ##

:of
    a                    lemon:SynRoleMarker ;
    lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
    lexinfo:partOfSpeech lexinfo:preposition .
