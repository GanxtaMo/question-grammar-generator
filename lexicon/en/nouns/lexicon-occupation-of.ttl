@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :occupation_of ;
  lemon:entry    :of .

:occupation_of a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :occupation_form ;
  lemon:synBehavior    :occupation_of_nounpp ;
  lemon:sense          :occupation_sense_ontomap .

:occupation_form a lemon:Form ;
  lemon:writtenRep "occupation"@en .

:occupation_of_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:occupation_sense_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :occupation_sense_ontomap ;
  lemon:reference           <http://dbpedia.org/ontology/occupation> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :occupation_condition .

:occupation_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/PersonFunction> .

:arg2 lemon:marker :of .

## Prepositions ##

:of a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .
