@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :birthDate_of ;
  lemon:entry    :of .

:birthDate_of a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :birthDate_form ;
  lemon:synBehavior    :birthDate_of_nounpp ;
  lemon:sense          :birthDate_sense_ontomap .

:birthDate_form a lemon:Form ;
  lemon:writtenRep "birth date"@en .

:birthDate_of_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:birthDate_sense_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :birthDate_sense_ontomap ;
  lemon:reference           <http://dbpedia.org/ontology/birthDate> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :birthDate_condition .

:birthDate_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange   <https://www.w3.org/2001/XMLSchema#date> .

:arg2 lemon:marker :of .

## Prepositions ##

:of a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .
