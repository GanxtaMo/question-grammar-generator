@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_build ;
  lemon:entry    :built ;
  lemon:entry    :by .

:to_build a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_build ;
  lemon:otherForm      :form_built ;
  lemon:synBehavior    :build_frame ;
  lemon:sense          :build_ontomap .

:form_build a          lemon:Form ;
  lemon:writtenRep     "build"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_built  a      lemon:Form ;
  lemon:writtenRep "built"@en ;
  lexinfo:tense    lexinfo:past .


:build_frame a                  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :build_subj ;
  lexinfo:prepositionalAdjunct :build_obj .

:build_ontomap a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :build_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/beginningDate> ;
  lemon:subjOfProp  :build_obj ;
  lemon:objOfProp   :build_subj ;
  lemon:condition   :build_condition .

:build_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/HistoricPlace> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/date> .

:build_obj lemon:marker :by .

## Prepositions ##

:by a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .
