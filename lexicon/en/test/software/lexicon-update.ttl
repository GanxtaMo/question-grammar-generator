@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base   <http://localhost:8080#> .

:lexicon_en
    a              lemon:Lexicon ;
    lemon:language "en" ;
    lemon:entry    :to_update ;
    lemon:entry    :by .

:to_update
    a                    lemon:LexicalEntry ;
    lexinfo:partOfSpeech lexinfo:verb ;
    lemon:canonicalForm  :form_update ;
    lemon:otherForm      :form_updates ;
    lemon:otherForm      :form_updated ;
    lemon:synBehavior    :update_frame_transitive, :update_frame_intransitive ;
    lemon:sense          :update_ontomap .

:form_update
    a                    lemon:Form ;
    lemon:writtenRep     "frequently update"@en ;
    lexinfo:verbFormMood lexinfo:infinitive .

:form_updates
    a                lemon:Form ;
    lemon:writtenRep "frequently updates"@en ;
    lexinfo:tense    lexinfo:present ;
    lexinfo:number   lexinfo:singular ;
    lexinfo:person   lexinfo:secondPerson .

:form_updated
    a                lemon:Form ;
    lemon:writtenRep "frequently updated"@en ;
    lexinfo:tense    lexinfo:past .


:update_frame_intransitive
    a                            lexinfo:IntransitivePPFrame ;
    lexinfo:subject              :update_subj ;
    lexinfo:prepositionalAdjunct :update_obj .

:update_frame_transitive
    a                            lexinfo:TransitivePPFrame ;
    lexinfo:subject              :update_subj ;
    lexinfo:prepositionalAdjunct :update_obj .

:update_ontomap
    a                 lemon:OntoMap, lemon:LexicalSense ;
    lemon:ontoMapping :update_ontomap ;
    lemon:reference   <https://dbpedia.org/ontology/frequentlyUpdated> ;
    lemon:subjOfProp  :update_obj ;
    lemon:objOfProp   :update_subj ;
    lemon:condition   :update_condition .

:update_condition
    a                    lemon:condition ;
    lemon:propertyDomain <https://dbpedia.org/ontology/Software> ;
    lemon:propertyRange  <https://www.w3.org/2001/XMLSchema#string> .

:update_obj
    lemon:marker :by .

## Prepositions ##

:by
    a                    lemon:SynRoleMarker ;
    lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
    lexinfo:partOfSpeech lexinfo:preposition .
