@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base   <http://localhost:8080#> .

:lexicon_en
    a              lemon:Lexicon ;
    lemon:language "en" ;
    lemon:entry    :to_participate ;
    lemon:entry    :in .

:to_participate
    a                    lemon:LexicalEntry ;
    lexinfo:partOfSpeech lexinfo:verb ;
    lemon:canonicalForm  :form_participate ;
    lemon:otherForm      :form_participates ;
    lemon:otherForm      :form_participated ;
    lemon:synBehavior    :participate_frame_transitive, :participate_frame_intransitive ;
    lemon:sense          :participate_ontomap .

:form_participate
    a                    lemon:Form ;
    lemon:writtenRep     "participate"@en ;
    lexinfo:verbFormMood lexinfo:infinitive .

:form_participates
    a                lemon:Form ;
    lemon:writtenRep "participates"@en ;
    lexinfo:tense    lexinfo:present ;
    lexinfo:number   lexinfo:singular ;
    lexinfo:person   lexinfo:secondPerson .

:form_participated
    a                lemon:Form ;
    lemon:writtenRep "participated"@en ;
    lexinfo:tense    lexinfo:past .


:participate_frame_intransitive
    a                            lexinfo:IntransitivePPFrame ;
    lexinfo:subject              :participate_subj ;
    lexinfo:prepositionalAdjunct :participate_obj .

:participate_frame_transitive
    a                            lexinfo:TransitivePPFrame ;
    lexinfo:subject              :participate_subj ;
    lexinfo:prepositionalAdjunct :participate_obj .

:participate_ontomap
    a                 lemon:OntoMap, lemon:LexicalSense ;
    lemon:ontoMapping :participate_ontomap ;
    lemon:reference   <https://dbpedia.org/ontology/isPartOfMilitaryConflict> ;
    lemon:subjOfProp  :participate_obj ;
    lemon:objOfProp   :participate_subj ;
    lemon:condition   :participate_condition .

:participate_condition
    a                    lemon:condition ;
    lemon:propertyDomain <https://dbpedia.org/ontology/MilitaryConflict> ;
    lemon:propertyRange  <https://dbpedia.org/ontology/MilitaryConflict> .

:participate_obj
    lemon:marker :in .

## Prepositions ##

:in
    a                    lemon:SynRoleMarker ;
    lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
    lexinfo:partOfSpeech lexinfo:preposition .
