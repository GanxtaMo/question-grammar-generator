@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base   <http://localhost:8080#> .

:lexicon_en
    a              lemon:Lexicon ;
    lemon:language "en" ;
    lemon:entry    :to_locate ;
    lemon:entry    :in .

:to_locate
    a                    lemon:LexicalEntry ;
    lexinfo:partOfSpeech lexinfo:verb ;
    lemon:canonicalForm  :form_locate ;
    lemon:otherForm      :form_locates ;
    lemon:otherForm      :form_locateed ;
    lemon:synBehavior    :locate_frame_transitive, :locate_frame_intransitive ;
    lemon:sense          :locate_ontomap .

:form_locate
    a                    lemon:Form ;
    lemon:writtenRep     "locate"@en ;
    lexinfo:verbFormMood lexinfo:infinitive .

:form_locates
    a                lemon:Form ;
    lemon:writtenRep "locates"@en ;
    lexinfo:tense    lexinfo:present ;
    lexinfo:number   lexinfo:singular ;
    lexinfo:person   lexinfo:secondPerson .

:form_located
    a                lemon:Form ;
    lemon:writtenRep "located"@en ;
    lexinfo:tense    lexinfo:past .


:locate_frame_intransitive
    a                            lexinfo:IntransitivePPFrame ;
    lexinfo:subject              :locate_subj ;
    lexinfo:prepositionalAdjunct :locate_obj .

:locate_frame_transitive
    a                            lexinfo:TransitivePPFrame ;
    lexinfo:subject              :locate_subj ;
    lexinfo:prepositionalAdjunct :locate_obj .

:locate_ontomap
    a                 lemon:OntoMap, lemon:LexicalSense ;
    lemon:ontoMapping :locate_ontomap ;
    lemon:reference   <https://dbpedia.org/ontology/Place> ;
    lemon:subjOfProp  :locate_obj ;
    lemon:objOfProp   :locate_subj ;
    lemon:condition   :locate_condition .

:locate_condition
    a                    lemon:condition ;
    lemon:propertyDomain <https://dbpedia.org/ontology/School> ;
    lemon:propertyRange  <https://dbpedia.org/ontology/Place> .

:locate_obj
    lemon:marker :in .

## Prepositions ##

:in
    a                    lemon:SynRoleMarker ;
    lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
    lexinfo:partOfSpeech lexinfo:preposition .
